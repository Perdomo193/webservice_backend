{
	"name": "webservicevo",
	"version": "1.0.0",
	"description": "Webservice for load image with opencvlirary",
	"main": "index.js",
	"scripts": {
		"start": "nodemon server.js",
		"start-dev": "node server.js",
		"migrate": "npx sequelize-cli db:migrate",
		"migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate && npx sequelize-cli db:seed:all  ",
		"test": "cross-env NODE_ENV=test jest  --forceExit --json",
		"pretest": "cross-env NODE_ENV=test npm run migrate:reset"
	},
	"jest": {
		"testEnvironment": "node",
		"coveragePathIgnorePatterns": [
			"/node_modules/"
		]
	},
	"keywords": [],
	"author": "perdomo192",
	"license": "ISC",
	"dependencies": {
		"body-parser": "^1.19.0",
		"@vue/cli-plugin-babel": "^4.5.9",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"sequelize": "^6.3.5",
		"sqlite3": "^5.0.0",
		"multer": "^1.4.2"
	},
	"devDependencies": {
		"cross-env": "^7.0.2",
		"jest": "^26.6.3",
		"nodemon": "^2.0.6",
		"sequelize-cli": "^6.2.0",
		"supertest": "^6.0.1"
	}
}
